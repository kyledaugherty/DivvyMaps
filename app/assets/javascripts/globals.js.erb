window.map = "";

function getOrigin(){
  var lat = $('#originLatitude');
  var lng = $('#originLongitude');
  if (lat !== "" && lng !== "")
    return  Marker(lat, lng, "start", createLocationIcon("start"));
  else{
    return $('#origin').val();
  }
};

var timeUp;

var changeTimeUp = function() {
  timeUp == true ? timeUp = false : timeUp = true;
  console.log(timeUp);
}

var startMinuteTimer = function() {
  timeUp = false;
  setTimeout(function(){ changeTimeUp(); }, 60000);
}
